{
  "mcpServers": {
    "nx-mcp": {
      "url": "http://localhost:9953/sse"
    },
    "langgraph-docs-mcp": {
      "command": "uvx",
      "args": [
        "--from",
        "mcpdoc",
        "mcpdoc",
        "--urls",
        "LangGraph:https://langchain-ai.github.io/langgraphjs/llms.txt LangChain:https://js.langchain.com/llms.txt",
        "--transport",
        "stdio"
      ]
    },
    "@magicuidesign/mcp": {
      "command": "pnpx",
      "args": ["-y", "@magicuidesign/mcp@latest"]
    },
    "shadcn": {
      "description": "",
      "type": "stdio",
      "command": "pnpx",
      "args": [
        "-y",
        "shadcn@canary",
        "registry:mcp"
      ],
      "env": {
        "REGISTRY_URL": "https://raw.githubusercontent.com/shadcn-ui/ui/refs/heads/main/apps/v4/registry.json"
      }
    }
  }
}
