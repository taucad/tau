{
  "name": "@taucad/units",
  "version": "0.1.0",
  "description": "A library for working with units of measurement and conversions",
  "type": "module",
  "private": true,
  "exports": {
    ".": {
      "types": "./src/types/index.ts",
      "import": "./src/types/index.ts",
      "default": "./src/types/index.ts"
    },
    "./constants": {
      "types": "./src/constants/index.ts",
      "import": "./src/constants/index.ts",
      "default": "./src/constants/index.ts"
    },
    "./converter": {
      "types": "./src/converter/index.ts",
      "import": "./src/converter/index.ts",
      "default": "./src/converter/index.ts"
    },
    "./parser": {
      "types": "./src/parser/index.ts",
      "import": "./src/parser/index.ts",
      "default": "./src/parser/index.ts"
    }
  },
  "imports": {
    "#*": "./src/*"
  }
}
